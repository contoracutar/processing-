  };
  public float GetPosX(){ return posX; }
  public float GetPosY(){ return posY; }
  public int BlockWidth = BlockData[0].length;
  public int BlockHeight = BlockData.length;
  private float posX, posY;
  private float scale;
  
  //  BlockConstructor
  CBlock(float posX, float posY, float scale)
  {
    this.posX = posX;
    this.posY = posY;
    this.scale = scale;
  }
  //  BlockDraw
  public void Draw()
  {
    translate(posX, posY);
    stroke(0);
    for(int x = 0; x < BlockWidth; x++)
    {
      for(int y = 0; y < BlockHeight; y++)
      {
        switch(BlockData[y][x])
        {
          case 0 : continue;
          case 1 : fill(255,0,0);break;
          case 2 : fill(0,255,0);break;
          case 3 : fill(0,0,255);break;
          //case '#':fill(0,0,255);break;
        }
        rect(x * scale, y * scale, scale, scale);
      }
    }
  } 
  public void Breake(int dateX,int dateY)
  {
  if(BlockData[dateX][dateY] > 0)BlockData[dateX][dateY] = 0;
  }
}
