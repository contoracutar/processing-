ArrayList BallList;
CPlayer player;
CBlock block;

float MovePosX;

//    Main     //
void setup()
{
  size(800, 1000);
  smooth();
  BallList = new ArrayList();
  player = new CPlayer(width / 2, 100.0f, 10.0f);
  block = new CBlock(50.0f, 50.0f, 10.0f);
}

void draw()
{
  noStroke();
  fill(0, 0, 0, 10);
  rect(0, 0, width, height);
  
  if (mousePressed)
  {
    BallList.add(new CBall(mouseX, mouseY, 5.0f, random(-5.0f, 5.0f), random(-5.0f, 5.0f)));
  }
  
  for (int i = BallList.size() - 1; i >= 0; i--)
  {
    CBall ball = (CBall) BallList.get(i);
    ball.Update(block);
    ball.Draw();
    //  player collision
    if (player.GetPosX() < ball.GetPosX() && ball.GetPosX() < player.GetPosX() + 100.0f
    && player.GetPosY() - 5 < ball.GetPosY() && ball.GetPosY() < player.GetPosY() + 5)
    {
      ball.SetVelocityY(ball.GetVelocityY() * -1);
    }
    //  block collision
/*    if (block.GetPosX() < ball.GetPosX() && ball.GetPosX() < block.GetPosX() + 50.0f)
    {
      ball.SetVelocityX(ball.GetVelocityX() * -1);
    }
*/    
    if (ball.GetisDelete())
    {
      BallList.remove(i);
    }
  }
  player.Update();
  player.Draw();
  block.Draw();
}

void mousePressed()
{
  MovePosX = player.GetPosX() - mouseX;
}

void mouseDragged()
{
  player.SetPosX(MovePosX + mouseX); 
}


//  PlayerClass  //
class CPlayer
{
  public void SetPosX(float x){ posX = x; }
  public float GetPosX(){ return posX; }
  public float GetPosY(){ return posY; }
  private float posX, posY = 900.0f;
  private float scaleX, scaleY;
  
  //  PlayerConstructor
  CPlayer (float posX, float scaleX, float scaleY)
  {
    this.posX = posX;
    this.scaleX = scaleX;
    this.scaleY = scaleY;    
  }
  //  PlayerUpdate
  public void Update()
  {
    if (posX < 0)
    {
      posX = 0;
    }
    else if (posX > width - 100)
    {
      posX = width - 100;
    }
  }
  //  PlayerDraw
  public void Draw()
  {
    fill(255);
    rect(posX, posY, scaleX, scaleY);
  }
}


//  BallClass  //
class CBall
{
  public void SetVelocityX(float x){ velocityX = x; }
  public void SetVelocityY(float y){ velocityY = y; }
  public float GetPosX(){ return posX; }
  public float GetPosY(){ return posY; }
  public float GetVelocityX(){ return velocityX; }
  public float GetVelocityY(){ return velocityY; }
  public boolean GetisDelete(){ return isDelete; }
  private float posX, posY;
  private float oldposX, oldposY;
  private float velocityX, velocityY;
  private float scale;
  private boolean isDelete;
  
  //  BallConstructor
  CBall (float posX, float posY, float scale, float velocityX, float velocityY)
  {
    this.posX = posX;
    this.posY = posY;
    this.velocityX = velocityX;
    this.velocityY = velocityY;
    this.scale = scale;
  }
  //  BallUpdate
  public void Update(CBlock block) 
  {
    oldposX = posX;
    oldposY = posY;
    posX += velocityX;
    posY += velocityY;
    
    for(int x = 0; x < block.BlockHeight; x++)
    {
      for(int y = 0; y < block.BlockWidth; y++)
      {
    
//----------------------
/*        if (block.BlockHeight * y + block.GetPosX() <= posX + 5 && posX + 5 <= block.BlockHeight * block.GetPosX() + (y + 1) + 10.0f)
        {
        //  velocityY *= -1;
        }
        if (block.GetPosY() + x * block.BlockHeight <= posY + 5 && posY + 5 <= block.GetPosY() + (x + 1) * block.BlockHeight
        && block.GetPosX() + y * block.BlockWidth <= posX + 5 && posX + 5 <= block.GetPosX() + y * block.BlockWidth)
        {
//          velocityX *= -1;
        }*/
//----------------------
        
        if(block.BlockData[x][y] == 0)continue;
        if (posY + 5 >= block.GetPosY() && posY <= block.GetPosY() + 5 && posX + 5 >= block.GetPosX() && posX <= block.GetPosX() + 10)
        {
          if (posX + 5 >= block.GetPosX() && posX <= block.GetPosX() + 10)
          {
            velocityY *= -1;
            //block.Breake(x, y);
          }
          if(posY + 5 >= block.GetPosY() && posY <= block.GetPosY() + 10)
          {
            velocityX *= -1;
            //block.Breake(x, y);
          }
        }
      }
    }
    if (posX < 0 || posX > width)
    { 
      velocityX *= -1;
    }
    else if (posY < 0)
    {
      velocityY *= -1;
    }
    if (posY > height)
    {
      isDelete = true;
    }
  }
  //  BallDraw
  public void Draw()
  {
    fill(255);
    ellipse(posX, posY, scale, scale);
  }
}


